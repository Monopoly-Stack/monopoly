<div class="container">
    <div class="row">
        <h1>{{title}}</h1>
        <p>Welcome to {{title}}</p>
        <div class="row">
            <div class="col-sm-12 col-md-6" id="game-field">
                <svg id="svg" width=600px height=600px></svg>
                <img id="piece1" src="./assets/img/boot.svg" />
            </div>
            <div class="col-sm-12 col-md-3" id="player-field">
                <div id="prop-info"></div>
                <div id="buy-opt"></div>
                <span><button type="submit" class="btn btn-default" id="submit">Submit</button></span>
                <span><button type="button" class="btn btn-default hidden" id="roll">Roll</button></span>
                <p id="roll" class="hidden">Test</p>
                <span><button type="submit" class="btn btn-default" id="start">Start Game</button></span>
                <svg id="dice-svg" width=400px height=80px></svg>
            </div>
        </div>
    </div>
</div>
